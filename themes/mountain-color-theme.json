{
  "$schema": "vscode://schemas/color-theme",
  "name": "Mountain",
  "type": "dark",
  "colors": {
    "focusBorder": "#535353",
    "foreground": "#d8d8d8",
    "widget.shadow": "#090909",
    "activityBar.background": "#0f0f0f",
    "activityBar.foreground": "#d8d8d8",
    "badge.background": "#8aac8b",
    "button.background": "#191919",
    "dropdown.background": "#151515",
    "editor.background": "#0f0f0f",
    "editor.foreground": "#d8d8d8",
    "editor.lineHighlightBackground": "#151515",
    "editorCursor.foreground": "#d8d8d8",
    "editorLineNumber.foreground": "#535353",
    "editorWhitespace.foreground": "#2a2a2a",
    "input.background": "#151515",
    "list.activeSelectionBackground": "#222222",
    "menu.background": "#151515",
    "panel.background": "#090909",
    "sideBar.background": "#131313",
    "statusBar.background": "#0f0f0f",
    "statusBar.foreground": "#d8d8d8",
    "tab.activeBackground": "#191919",
    "tab.inactiveBackground": "#131313",
    "titleBar.activeBackground": "#0f0f0f"
  },
  "tokenColors": [
    {
      "scope": ["variable", "variable.parameter", "variable.other"],
      "settings": { "foreground": "#d8d8d8" }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#535353", "fontStyle": "italic" }
    },
    {
      "scope": ["string", "string.quoted", "string.regexp"],
      "settings": { "foreground": "#8aac8b" }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": { "foreground": "#d8bb92" }
    },
    {
      "scope": [
        "entity.name.function",
        "entity.name.tag",
        "entity.name.type",
        "support.function"
      ],
      "settings": { "foreground": "#a5a0c2" }
    },
    {
      "scope": [
        "keyword",
        "storage",
        "storage.type",
        "keyword.control",
        "keyword.operator"
      ],
      "settings": { "foreground": "#ac8aac" }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#b18f91", "fontStyle": "underline italic" }
    },
    {
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": { "foreground": "#91b2b3" }
    },
    {
      "scope": "markup.heading",
      "settings": { "foreground": "#99bb9a", "fontStyle": "bold" }
    },
    {
      "scope": "markup.italic",
      "settings": { "fontStyle": "italic" }
    },
    {
      "scope": "markup.bold",
      "settings": { "fontStyle": "bold" }
    }
  ]
}
